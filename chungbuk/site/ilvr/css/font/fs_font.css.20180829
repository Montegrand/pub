	font-weight:200;
	font-style:normal;
	font-family:'Nanum Square';
	src: url('NanumSquareLight.eot');
	src: local('나눔스퀘어 Light'), local('NanumSquareL'), local('나눔스퀘어 Light'), url('NanumSquareLight.eot?#iefix') format("embedded-opentype"),
	url('NanumSquareLight.woff') format("woff"),
	url('NanumSquareLight.ttf') format("truetype");
}
@font-face {
	font-weight:400;
	font-style:normal;
	font-family:'Nanum Square';
