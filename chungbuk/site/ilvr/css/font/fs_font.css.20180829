	url('NanumSquareBold.woff') format("woff"),
	url('NanumSquareBold.ttf') format("truetype");
}
@font-face {
	font-weight:800;
	font-style:normal;
	font-family:'Nanum Square';
	src: url('NanumSquareExtrabold.eot');
	src: local('나눔스퀘어 ExtraBold'), local('NanumSquareEB'), url('NanumSquareExtrabold.eot?#iefix') format("embedded-opentype"),
	url('NanumSquareExtrabold.woff') format("woff"),
	url('NanumSquareExtrabold.ttf') format("truetype");
